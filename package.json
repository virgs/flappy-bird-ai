  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "expose-loader": "^4.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.4.2",
    "tslint": "^6.1.2",
    "typescript": "^3.7.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virgs/flappy-bird-ai.git"
  },
  "author": {
    "name": "virgs",
    "email": "guilherme.moraes@outlook.com"
  },
  "bugs": {
    "url": "https://github.com/virgs/flappy-bird-ai/issues"
  },
  "homepage": "https://github.com/virgs/flappy-bird-ai#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
